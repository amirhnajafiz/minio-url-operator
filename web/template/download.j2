<h2>
downloading file ...
</h2>

<script>
    async function download(url) {
        await fetch(url)
            .then( res => res.blob() )
            .then( blob => {
                let file = window.URL.createObjectURL(blob);
                window.location.assign(file);
            })
            .catch(error => {
                console.log(error);
                alert("Failed to download!");
            });
    }

    download("{{ address }}");
</script>
